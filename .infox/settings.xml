<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
  <profiles>
    <profile>
      <id>epp</id>
      <repositories>
        <repository>
          <id>3rdparty</id>
          <url>http://ci.infox.intranet:8081/nexus/content/groups/non-local/</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
        </repository>
        <repository>
          <id>localfs</id>
          <url>file://${project.basedir}/repo</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <snapshots>
            <enabled>false</enabled>
          </snapshots>
          <id>nexus-plugin-releases</id>
          <name>repo</name>
          <url>http://ci.infox.intranet:8081/nexus/content/groups/non-local/</url>
        </pluginRepository>
        <pluginRepository>
          <snapshots/>
          <id>nexus-plugin-snapshots</id>
          <name>repo</name>
          <url>http://ci.infox.intranet:8081/nexus/content/groups/non-local/</url>
        </pluginRepository>
      </pluginRepositories>
      <properties>
        <downloadSources>true</downloadSources>
        <downloadJavadocs>true</downloadJavadocs>
      </properties>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>epp</activeProfile>
  </activeProfiles>
</settings>
