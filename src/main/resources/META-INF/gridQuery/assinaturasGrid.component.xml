<components xmlns="http://jboss.com/products/seam/components" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jboss.com/products/seam/core http://jboss.com/products/seam/core-2.2.xsd                   http://jboss.com/products/seam/security http://jboss.com/products/seam/security-2.2.xsd                  http://jboss.com/products/seam/components http://jboss.com/products/seam/components-2.2.xsd">	

<!-- $Id: assinaturasGrid.component.xml 815 2010-09-16 15:01:33Z laercio $ -->


	<component name="assinaturasGrid" class="br.com.itx.component.grid.GridQuery">
		<property name="ejbql">select o from ProcessoDocumento o where
							   o.processoDocumentoBin.certChain is not null
		</property>
		<property name="maxResults">16</property>
		<property name="gridId">assinaturas</property>
		<property name="key">idProcessoDocumento</property>
		
		<property name="columns">			
			<value>processoDocumentoBin.usuario: {properties:{header: #{messages['assinaturasGrid.usuario']}}}</value>		
			<value>processoDocumentoBin.dataInclusao: {properties:{header: #{messages['assinaturasGrid.dataInclusao']}}}</value>	
		</property>
		
		<property name="restrictions">
			<value>idProcessoDocumento = #{processoDocumentoHome.instance.idProcessoDocumento}</value>
		</property>
				
		<property name="order">processoDocumentoBin.dataInclusao</property>
	</component>
	
</components>
