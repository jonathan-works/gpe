<?xml version="1.0" encoding="UTF-8"?>

<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      login-required="true">
	 
	<description>Status: #{statusHome.instance}</description>
	
	<begin-conversation join="true"/>
	<param name="status.order" value="#{statusGrid.order}"/>
	<param name="id" value="#{statusHome.statusIdStatus}"/>

<!-- Parametros para RESTFull -->
	<param name="tab" value="#{statusHome.tab}"/>
	<param name="goBackUrl" value="#{statusHome.goBackUrl}"/>
	<param name="goBackId" value="#{statusHome.goBackId}"/>
	<param name="goBackTab" value="#{statusHome.goBackTab}"/>		

	<action if="#{empty statusHome.tab}" execute="#{statusHome.setTab('search')}"/>
</page>