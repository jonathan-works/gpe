<?xml version="1.0" encoding="UTF-8"?>

<page xmlns="http://jboss.org/schema/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	  xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">

	<begin-conversation join="true" />
	<restrict>#{security.checkPage()}</restrict>

	<param name="order" value="#{tipoProcessoDocumentoList.order}" />
	<param name="restrictionLogicOperator" value="#{tipoProcessoDocumentoList.restrictionLogicOperator}" />
	
	<param name="codigoDocumento" value="#{tipoProcessoDocumentoList.entity.codigoDocumento}"/>
	<param name="tipoProcessoDocumento" value="#{tipoProcessoDocumentoList.entity.tipoProcessoDocumento}"/>
	<param name="inTipoDocumento" value="#{tipoProcessoDocumentoList.entity.inTipoDocumento}"/>
	<param name="visibilidade" value="#{tipoProcessoDocumentoList.entity.visibilidade}"/>
	<param name="ativo" value="#{tipoProcessoDocumentoList.entity.ativo}"/>

</page>