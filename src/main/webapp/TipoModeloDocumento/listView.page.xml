<?xml version="1.0" encoding="UTF-8"?>

<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
	  login-required="true"
>
	<restrict>#{security.checkPage()}</restrict>
	<description>TipoModeloDocumento: #{tipoModeloDocumentoHome.instance}</description>
	
	<begin-conversation join="true"/>
	<param name="id" value="#{tipoModeloDocumentoHome.tipoModeloDocumentoIdTipoModeloDocumento}"/>

<!-- Parametros para RESTFull -->
	<param name="tab" value="#{tipoModeloDocumentoHome.tab}"/>
	<param name="goBackUrl" value="#{tipoModeloDocumentoHome.goBackUrl}"/>
	<param name="goBackId" value="#{tipoModeloDocumentoHome.goBackId}"/>
	<param name="goBackTab" value="#{tipoModeloDocumentoHome.goBackTab}"/>		
	<param name="grupoModeloDocumento" value="#{grupoModeloDocumentoSearch.idGrupoModeloDocumento}"/>			
	<param name="tipoModeloDocumento" value="#{tipoModeloDocumentoSearch.tipoModeloDocumento}"/>		
	<param name="abreviacao" value="#{tipoModeloDocumentoSearch.abreviacao}"/>		

	<action if="#{empty tipoModeloDocumentoHome.tab}" execute="#{tipoModeloDocumentoHome.setTab('search')}"/>
	<action if="#{param.newInstance == 'true'}" execute="#{variavelHome.newInstance()}"/>
	<!-- Parametros para a ordem de grids -->
 
</page>