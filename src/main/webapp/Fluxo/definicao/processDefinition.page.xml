<?xml version="1.0" encoding="UTF-8"?>

<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd">

	<begin-conversation join="true" />
	<param name="id" value="#{processBuilder.id}"/>
	<param name="node" value="#{nodeFitter.nodeIndex}"/>
	<param name="tab" value="#{processBuilder.tab}"/>
	<action execute="#{processBuilder.getPaintedGraph()}" if="#{processBuilder.tab eq 'graphTab'}"/>
	<action execute="#{variavelProcessoAction.setFluxo(fluxoCrudAction.instance)}" on-postback="false"/>
</page>