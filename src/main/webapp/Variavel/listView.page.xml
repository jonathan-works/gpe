<?xml version="1.0" encoding="UTF-8"?>

<page xmlns="http://jboss.org/schema/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.org/schema/seam/pages http://jboss.org/schema/seam/pages-2.3.xsd"
	  login-required="true">
	  
	<description>Variavel: #{variavelCrudAction.instance}</description>
	<restrict>#{security.checkPage()}</restrict>
	<begin-conversation join="true"/>


	<param name="id" value="#{variavelCrudAction.id}" converterId="integerConverter"/>
	<param name="tab" value="#{variavelCrudAction.tab}"/>
	
	<action execute="#{associatedTipoModeloVariavelList.entity.setVariavel(variavelCrudAction.instance)}" 
            if="#{variavelCrudAction.tab eq 'tipoModelo'}"/>
    <action execute="#{associativeTipoModeloDocumentoList.setVariavelToIgnore(variavelCrudAction.instance)}" 
            if="#{variavelCrudAction.tab eq 'tipoModelo'}"/>
    <action execute="#{variavelTipoModeloDocumentoCrudAction.setVariavelAtual(variavelCrudAction.instance)}" 
            if="#{variavelCrudAction.tab eq 'tipoModelo'}"/>
</page>