      xsi:schemaLocation="http://www.jboss.org/schema/seam/pages http://www.jboss.org/schema/seam/pages-2.3.xsd"
	  login-required="true"
>
	
	<description>Processo: #{processoHome.instance}</description>
	
	<begin-conversation join="true"/>

<!-- Parametros para RESTFull -->
	<param name="tab" value="#{processoHome.tab}"/>
	<param name="goBackUrl" value="#{processoHome.goBackUrl}"/>
	<param name="goBackId" value="#{processoHome.goBackId}"/>
	<param name="goBackTab" value="#{processoHome.goBackTab}"/>		

	<param name="idBin" value="#{processoDocumentoBinHome.processoDocumentoBinIdProcessoDocumentoBin}"/>
	<param name="idProcesso" value="#{processoHome.processoIdProcesso}"/>	
	<action execute="#{processoHome.iniciarTarefaProcesso()}"/>	
 
</page>